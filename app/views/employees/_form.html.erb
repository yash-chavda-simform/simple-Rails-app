<%= form_with model: @employee do |form| %>
  <%= form.hidden_field :lock_version %>
  <div>
    <%= form.label :first_name,class:"col-sm-2 col-form-label" %><br>
    <%= form.text_field :first_name , class:"form-control" %>
  </div>
  <div>
    <%= form.label :last_name, class:"col-sm-2 col-form-label" %><br>
    <%= form.text_field :last_name, class:"form-control" %>
  </div>
  <div>
    <%= form.label :email, class:"col-sm-2 col-form-label" %><br>
    <%= form.text_field :email, class:"form-control" %>
  </div>
  <div>
    <%= form.label :age, class:"col-sm-2 col-form-label" %><br>
    <%= form.text_field :age, class:"form-control" %>
  </div>
  <div>
    <%= form.label :no_of_order, class:"col-sm-2 col-form-label" %><br>
    <%= form.text_field :no_of_order, class:"form-control" %>
  </div>
  <div>
    <%= form.label :full_time_available, class:"col-sm-2 col-form-label" %><br>
    <%= form.text_field :full_time_available, class:"form-control" %>
  </div>
  <div>
    <%= form.label :salary, class:"col-sm-2 col-form-label" %><br>
    <%= form.text_field :salary, class:"form-control" %>
  </div>
  <br>
  <div>
    <%= form.submit "Click",class:"btn btn-primary" %>
  </div>
<%end%>
