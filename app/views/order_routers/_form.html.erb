<div class="container">
  <%= form_with(model: @order, url: target) do |form| %>
    <div>
      <%= form.label :quantity,class:"col-sm-2 col-form-label" %><br>
      <%= form.text_field :quantity, class:"form-control"%>
    </div>
    <br>
    <div>
      <%= form.submit class:"btn btn-primary" %>
    </div>
  <% end %>
</div>
